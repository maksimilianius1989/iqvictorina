    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff5ea75a" android:endColor="#ff2d7725" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff407725" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
</selector>