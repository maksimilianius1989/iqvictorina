<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff5ea75a" android:endColor="#ff5ea75a" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff254d77" />
